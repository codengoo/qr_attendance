{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*",
      ],
      "@components": [
        "src/components",
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@constants": [
        "src/constants"
      ],
    },
  }
}